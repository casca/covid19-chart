{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "(/(?!covid)(?!api).*)",
      "destination": "/covid"
    },
    {
      "source": "/covid/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/covid/((?!api/).*)",
      "destination": "/covid"
    }
  ]
}
